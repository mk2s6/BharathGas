{
  "name": "BharatGas-node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "devStart": "set DEBUG=barebone-node-project:* & nodemon ./bin/www",
    "test": "jest --detectOpenHandles --runInBand ./tests",
    "verboseTest": "jest --detectOpenHandles --verbose true --runInBand ./tests",
    "failFirstTest": "jest --detectOpenHandles --bail --verbose true --runInBand ./tests"
  },
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "./tests/global-setup.js",
    "globalTeardown": "./tests/global-teardown.js",
    "setupFiles": [
      "./tests/test-suite-setup.js"
    ],
    "testPathIgnorePatterns": [
      "./.vscode/",
      "./bin/",
      "./config/",
      "./node_modules/",
      "./public/",
      "./samples/",
      "./tools/"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.5",
    "cron": "^1.7.0",
    "date-and-time": "^0.6.3",
    "debug": "~2.6.9",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-hbs": "^2.1.2",
    "express-validator": "^5.3.0",
    "http-errors": "~1.6.2",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql-import": "^2.0.2",
    "mysql2": "^1.6.4",
    "nodemailer": "^5.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "password-validator": "^4.1.1",
    "pify": "^4.0.1",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "jest": "^24.5.0"
  }
}
