{{> header}}
{{> salesHeader}}

<section class="page_breadcrumbs ds color parallax section_padding_top_20 section_padding_bottom_20">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 text-center">
                <h2 class="text-uppercase">Register Customer</h2>
            </div>
        </div>
    </div>
</section>
<section class="ls section_padding_top_30 section_padding_bottom_30">
    <div class="container" align="center">
        <div class="row">
            <form class="shop-register" role="form">
                <div class="col-sm-6">
                    <div class="form-group validate-required" id="billing_first_name_field">
                        <span class="input input--hoshi">
                            <input class="input__field input__field--hoshi" type="text" name="ui_business_name"
                                id="ui_business_name" required />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1"
                                for="ui_business_name">
                                <span class="input__label-content input__label-content--hoshi">Business Name</span>
                            </label>
                        </span>
                    </div>
                    <div class="form-group validate-required" id="billing_last_name_field">
                        <span class="input input--hoshi">
                            <input class="input__field input__field--hoshi" type="text" name="ui_mobile" id="ui_mobile"
                                required />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1" for="ui_mobile">
                                <span class="input__label-content input__label-content--hoshi">Mobile</span>
                            </label>
                        </span>
                    </div>
                    <div class="form-group validate-required" id="billing_last_name_field">
                        <span class="input input--hoshi">
                            <input class="input__field input__field--hoshi" type="text" name="ui_secondary_mobile"
                                id="ui_secondary_mobile" required />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1"
                                for="ui_secondary_mobile">
                                <span class="input__label-content input__label-content--hoshi"> Secondary Mobile</span>
                            </label>
                        </span>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group validate-required" id="billing_first_name_field">
                        <span class="input input--hoshi">
                            <input class="input__field input__field--hoshi" type="text" name="ui_proprietor_name"
                                id="ui_proprietor_name" required />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1"
                                for="ui_proprietor_name">
                                <span class="input__label-content input__label-content--hoshi">Proprietor Name</span>
                            </label>
                        </span>
                    </div>
                    <div class="form-group validate-required" id="billing_last_name_field">
                        <span class="input input--hoshi">
                            <input class="input__field input__field--hoshi" type="email" name="ui_email"
                                id="ui_email" />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1" for="ui_email">
                                <span class="input__label-content input__label-content--hoshi">E-mail</span>
                            </label>
                        </span>
                    </div>
                    <div class="form-group"> <label for="ui_omc" class="control-label">
                            <select class="form-control" name="ui_omc" id="ui_omc" style="margin-top:15px; width:350px"
                                required>
                                <option value="" selected="selected">Select OMC</option>
                                <option value="BPC">BPC</option>
                                <option value="HPC">HPC</option>
                                <option value="IOC">IOC</option>
                                <option value="Super Gas">Super Gas</option>
                                <option value="Go Gas">Go Gas</option>
                                <option value="Total Gas">Total Gas</option>
                                <option value="Others">Others</option>
                            </select>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="col-lg-4 col-sm-12">
                        <button style="padding:21px; font-size:17px; margin-top:25px" type="button" id="geo_loc"
                            class="fa fa-map-marker theme_button wide_button color2">&nbsp; | &nbsp; Get
                            current
                            location</button>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-12">
                        <div class="form-group address-field validate-required"> <label for="ui_latitude"
                                class="control-label">
                                <span class="grey">Latitude</span>
                            </label> <input type="text" class="form-control dis_field" name="ui_latitude"
                                id="ui_latitude" placeholder="" value="" disabled>
                        </div>
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-12">
                        <div class="form-group address-field validate-required"> <label for="ui_longitude"
                                class="control-label">
                                <span class="grey">Longitude</span>
                            </label> <input type="text" class="form-control dis_field" name="ui_longitude"
                                id="ui_longitude" placeholder="" value="" disabled>
                        </div>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group address-field validate-required" id="billing_address_fields">
                        <span class="input input--hoshi" style="max-width:950px">
                            <input class="input__field input__field--hoshi" type="text" name="ui_address"
                                id="ui_address" required />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1"
                                for="ui_address">
                                <span class="input__label-content input__label-content--hoshi">Address</span>
                            </label>
                        </span>
                    </div>
                </div>
                <div class="col-sm-6">
                    <div class="form-group"> <label for="billing_state" class="control-label">
                            <select class="form-control" name="ui_state" id="ui_city"
                                style="margin-top:20px; width:350px" required>
                                <option value="" selected="selected">Select a city</option>
                                <option value="SriKalahasti" selected="">SriKalahasti</option>
                            </select>
                    </div>
                    <div class="form-group"> <label for="billing_state" class="control-label">
                            <select class="form-control" name="ui_state" id="ui_state"
                                style="margin-top:20px; width:350px" required>
                                <option value="" selected="selected">Select a state</option>
                                <option value="Andhra Pradesh" selected="">Andhra Pradesh</option>
                            </select>
                    </div>

                </div>
                <div class="col-sm-6">
                    <div class="form-group validate-required" id="billing_last_name_field">
                        <span class="input input--hoshi">
                            <input class="input__field input__field--hoshi" type="text" name="ui_pincode"
                                id="ui_pincode" required />
                            <label class="input__label input__label--hoshi input__label--hoshi-color-1"
                                for="ui_pincode">
                                <span class="input__label-content input__label-content--hoshi">Pincode</span>
                            </label>
                        </span>
                    </div>
                    <div class="form-group"> <label for="billing_country" class="control-label">
                            <select class="form-control" name="ui_country" id="ui_country" style="width:350px" required>
                                <option value="" selected="selected">Select a country</option>
                                <option value="India" selected="selected">India</option>
                            </select>
                    </div>
                </div>
                <div class="col-sm-12">
                    <div class="form-group address-field validate-required validate-postcode"> <label for="ui_feedback"
                            class="control-label">
                            <span style="color:#6a7989; font-weight:900">Feedback:</span>
                        </label> <textarea class="form-control" rows="4" cols="30" name="ui_feedback" id="ui_feedback"
                            style="resize:none; margin: 0px 92.5px;width:954px; height: 125px;" placeholder=""
                            value=""></textarea>
                    </div>
                </div>
                <div class="col-sm-12">
                    <button type="button" id="registerCustomer"
                        class="theme_button wide_button color1 topmargin_40">Register Now</button>
                    <button type="reset" class="theme_button wide_button">Clear Form</button> </div>
            </form>
        </div>
    </div>
</section>

<script src="js/classie.js"></script>
<script src="js/stable.js"></script>
<script src="js/customer.js"></script>
<script src="js/getLocation.js"></script>

{{> footer-end}}